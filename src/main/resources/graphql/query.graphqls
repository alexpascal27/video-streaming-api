type Query {
    videoSearch(tag: String): [Video!]
    videoById(id: ID!): Video!
}

type Video {
    id: String!,
    name: String!,
    duration: String!,
    iconURL: String!,
    thumbnailURL: String!,
    creatorName: String!,
    viewCount: String!,
    timeSincePublished: String!,
    isCreatorVerified: Boolean,
}



